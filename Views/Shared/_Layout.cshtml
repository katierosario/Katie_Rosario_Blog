<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Katie Rosario | Blog</title>
    <!-- Favicons-->
    <link rel="shortcut icon" href="~/Images/favicon.png">
    <link rel="apple-touch-icon" href="~/Images/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Images/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Images/apple-touch-icon-114x114.png">
    <!-- Web Fonts-->
    <link href="//fonts.googleapis.com/css?family=Lora|Montserrat:400,500,600,700|Open+Sans:400,400i,700" rel="stylesheet">
    <!-- Bootstrap core CSS-->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Icon Fonts-->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/et-line.css" rel="stylesheet" />
    <!-- Template core CSS-->
    <link href="~/Content/template.css" rel="stylesheet" />
    <link href="~/Content/Katies.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
</head>
<body>

    <!-- Preloader-->
    <div class="page-loader">
        <div class="cssload-container">
            <div class="cssload-whirlpool"></div>
        </div>
    </div>
    <!-- Preloader end-->
    <!-- Header-->
    <header class="header header-center">
        <div class="container-fluid">
            <!-- Brand-->
            <div class="inner-header"><a class="inner-brand" href="@Url.Action("Index", "Home")"><img class="brand-dark" src="~/Images/logo.png" width="30px" alt=""><img class="brand-light" src="~/Images/logo-light.png" width="30px" alt=""></a></div>
            <!-- Navigation-->
            <div class="inner-navigation collapse">
                <div class="inner-nav onepage-nav">
                    <ul>
                        <li><a href="@Url.Action("Index", "Home")"><span class="menu-item-span">Home</span></a></li>
                        <li><a href="@Url.Action("Contact", "Home")"><span class="menu-item-span">Contact</span></a></li>
                        @Html.Partial("_LoginPartial")
                    </ul>
                    
                </div>
            </div>
            <!-- Mobile menu-->
            <div class="nav-toggle"><a href="#" data-toggle="collapse" data-target=".inner-navigation"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></a></div>
        </div>
    </header>
    <!-- Header end-->
    <!-- Wrapper-->
    <div class="wrapper">

        <!-- Blog-->

        @RenderBody()

        <!-- Blog end-->
        <!-- Footer-->
        <footer class="footer">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-6"><span class="copyright">© 2020 <a href="https://katierosarioportfolio.azurewebsites.net/">Katie Rosario</a>, All Rights Reserved.</span></div>
                    <div class="col-md-6">
                        <ul class="social-icons">
                            <li><a href="https://www.linkedin.com/in/katie-rosario/"><i class="fa fa-linkedin-square"></i></a></li>
                            <li><a href="https://github.com/katierosario"><i class="fa fa-github-square"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!-- Footer end-->
    </div>
    <!-- Wrapper end-->
    <!-- Scripts-->
    <script src="~/Scripts/jquery-2.2.4.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/plugins.min.js"></script>
    <script src="~/Scripts/custom.min.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>

